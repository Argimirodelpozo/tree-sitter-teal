{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "teal",
      "camelcase": "Teal",
      "title": "Teal",
      "scope": "source.teal",
      "file-types": [
        "teal"
      ],
      "injection-regex": "^teal$",
      "class-name": "TreeSitterTeal"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree sitter parser for the TEAL DSL (Algorand Virtual Machine assembly language)",
    "authors": [
      {
        "name": "Argimiro del Pozo"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-teal"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}